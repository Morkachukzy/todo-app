,0BAQA,KARA,EAQA;AACA,WAAA,WAAA,GAAA,KAAA,CADA,CAEA;AACA,KAXA;AAYA,IAAA,qBAZA,iCAYA,KAZA,EAYA;AACA;AACA,aAAA,KAAA,WAAA,KAAA,KAAA;AACA,KAfA;AAgBA,IAAA,eAhBA,6BAgBA;AACA,WAAA,WAAA,GAAA,IAAA;AACA,KAlBA;AAmBA,IAAA,UAnBA,sBAmBA,WAnBA,EAmBA;AACA;AACA,WAAA,KAAA,CAAA,aAAA,EAAA,WAAA;AACA;AAtBA;AAjBA,CAAA","sourcesContent":["<template>\n  <div class=\"todo-list\">\n    <ul>\n      <li\n        v-for=\"(todo, index) in todos\"\n        :key=\"JSON.stringify(todo)\"\n        :data-index=\"index\"\n        @mouseover.stop=\"setHoveredTodo(index)\"\n        @mouseleave.stop=\"unsetSetHovered\"\n      >\n        <div class=\"checkbox\">\n          <base-check-box :currentTodo=\"todo\" @check-entry=\"checkEntry\" />\n        </div>\n        <span :class=\"[selectedCSS(todo), 'todo-content']\">\n          {{ todo.title }}\n        </span>\n        <div class=\"delete-button\" v-if=\"isHoveredTodoActiveAt(index)\">\n          <base-delete-button\n            :currentTodo=\"todo\"\n            :index=\"index\"\n            @delete-todo=\"deleteTodo\"\n          />\n        </div>\n      </li>\n    </ul>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: \"TheTodoList\",\n  props: {\n    todos: {\n      type: Array,\n    },\n  },\n  data() {\n    return {\n      isSelected: false,\n      selectedIndex: null,\n      displayDeleteButton: false,\n      hoveredTodo: null,\n    };\n  },\n  emits: [\"check-entry\", \"delete-todo\"],\n\n  methods: {\n    checkEntry(currentTodo) {\n      this.$emit(\"check-entry\", currentTodo);\n    },\n    selectedCSS(todo) {\n      // if()\n      return { \"cross-out\": !todo.active };\n    },\n    setHoveredTodo(index) {\n      this.hoveredTodo = index;\n      // console.log(\"CAlled\" + this.displayDeleteButton);\n    },\n    isHoveredTodoActiveAt(index) {\n      // console.log(\"CAlled 2\");\n      return this.hoveredTodo === index;\n    },\n    unsetSetHovered() {\n      this.hoveredTodo = null;\n    },\n    deleteTodo(currentTodo) {\n      // console.log(\"delete 3 called\"+ currentTodo);\n      this.$emit(\"delete-todo\", currentTodo);\n    },\n  },\n};\n</script>\n\n<style scoped src=\"@/assets/styles/Layout/TheTodoList.css\"></style>\n"],"sourceRoot":"src/components/Layout"}]}