log(\"mounted: \" + this.theme);\n  },\n  created() {\n    eventBus.$on(\"change-theme\", this.setSelectedTheme);\n    console.log(\"created: \" + this.theme);\n  },\n  watch: {\n    theme() {\n      document.documentElement.setAttribute(\"data-theme\", this.theme);\n      console.log(\"watcher: \" + this.theme);\n    },\n  },\n  computed: {\n    ...mapGetters([\"getCurrentTheme\"]),\n  },\n  methods: {\n    setSelectedTheme() {\n      this.theme = this.getCurrentTheme;\n    },\n  },\n};\n</script>\n\n<style sr>\n@import url(\"https://fonts.googleapis.com/css2?family=Josefin+Sans:wght@400;700&display=swap\");\nbody {\n  font-size: 18px;\n  margin: 0;\n  background-color: var(--background-color);\n  background-image: url(\"./assets/images/bg-desktop-light.jpg\");\n  background-repeat: no-repeat;\n  background-attachment: fixed;\n  background-position: top;\n  background-size: contain;\n\n  display: grid;\n  place-items: center;\n  box-sizing: border-box;\n}\nbody[data-theme=\"Dark\"] {\n  background-color: var(--background-color);\n\n  background-image: url(\"./assets/images/bg-desktop-dark.jpg\");\n  background-repeat: no-repeat;\n  background-attachment: fixed;\n  background-position: top;\n  background-size: contain;\n}\n#app {\n  font-family: \"Josefin Sans\", sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: var(--dark-grayish-blue);\n}\n\n\n\n:root {\n  /* Light Theme */\n  --very-light-gray: hsl(235, 19%, 35%);\n  --very-light-grayish-blue: hsl(236, 33%, 92%);\n  --light-grayish-blue: hsl(233, 11%, 84%);\n  --dark-grayish-blue: hsl(236, 9%, 61%);\n  --very-dark-grayish-blue: hsl(235, 19%, 35%);\n\n  --background-color: var(--very-dark-gray);\n  --box-shadow: 5px 10px var(--very-light-grayish-blue);\n  --default-button-font-color: var(--light-grayish-blue);\n  --section-button-font-color: var(--dark-grayish-blue);\n  --list-item-font-color: var(--dark-grayish-blue);\n}\n[data-theme=\"Dark\"] {\n  /* Dark Theme */\n  --very-dark-blue: hsl(235, 21%, 11%);\n  --very-dark-desaturated-blue: hsl(235, 24%, 19%);\n  --light-grayish-blue: hsl(234, 39%, 85%);\n  --light-grayish-blue-hover: hsl(236, 33%, 92%);\n  --dark-grayish-blue: hsl(234, 11%, 52%);\n  --very-dark-grayish-blue: hsl(233, 14%, 35%);\n  --second-very-dark-grayish-blue: hsl(237, 14%, 26%);\n\n  --background-color: var(--very-dark-blue);\n  --canceled-todo-font-color: var(--very-dark-desaturated-blue);\n  --list-item-font-color: var(--dark-grayish-blue);\n  --default-button-font-color: var(--light-grayish-blue);\n  --hover-font-color: var(--light-grayish-blue-hover);\n  --section-button-font-color: var(--very-dark-grayish-blue);\n  --todo-grid-background-color: var(--second-very-dark-grayish-blue);\n\n\n}\nhtml {\n  box-sizing: border-box;\n  margin: 0;\n}\n*,\n*::after,\n*::before {\n  box-sizing: inherit;\n}\n</style>\n"]}]}