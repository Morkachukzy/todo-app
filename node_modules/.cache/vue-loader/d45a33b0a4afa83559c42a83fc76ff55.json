Theme === \"Light\" ? \"Dark\" : \"Light\";\n    },\n  },\n  methods: {\n    switchTheme() {\n      this.selectedTheme = this.selectedTheme === \"Light\" ? \"Dark\" : \"Light\";\n      this.$emit(\"switch-theme\", this.selectedTheme);\n    },\n    setThemeIcon() {\n      this.selectedTheme === \"Light\"\n        ? (this.selectedThemeIcon = require(\"../../assets/icon-moon.svg\"))\n        : (this.selectedThemeIcon = require(\"../../assets/icon-sun.svg\"));\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]}]}